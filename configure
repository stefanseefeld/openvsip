            # confuses our option extraction below.  We use '-nocompchk' to
            # disable this command.
            if test "$enable_parallel" = "intelmpi"
            then
              command="$MPICXX -nocompchk -show -c | cut -d' ' -f2- | sed -e 's/ -c / /'"
