if test "${with_pas_include+set}" = set; then
  withval=$with_pas_include;   enable_parallel=pas
fi



# Check whether --with-pas_lib was given.
if test "${with_pas_lib+set}" = set; then
  withval=$with_pas_lib;   enable_parallel=pas
fi


# Check whether --enable-pas_heap_size was given.
if test "${enable_pas_heap_size+set}" = set; then
  enableval=$enable_pas_heap_size;
else
  enable_pas_heap_size=0x100000
fi


# Check whether --enable-pas_share_dynamic_xfer was given.
