  withval=$with_version_string; case "$withval" in
    yes) { { $as_echo "$as_me:$LINENO: error: version-string not specified" >&5
$as_echo "$as_me: error: version-string not specified" >&2;}
   { (exit 1); exit 1; }; } ;;
    no) ;;
    *) version_string="$withval"
       major_version_string="${withval%-*}"
       ;;
  esac
fi



cat >>confdefs.h <<_ACEOF
#define VSIP_IMPL_VERSION_STRING "$version_string"
_ACEOF



cat >>confdefs.h <<_ACEOF
#define VSIP_IMPL_MAJOR_VERSION_STRING "$major_version_string"
_ACEOF



